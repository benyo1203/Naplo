<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.naplo.adatbazis.OlvasController">
    <TableView fx:id="dataTable" prefHeight="400.0" prefWidth="600.0">
        <columns>
            <TableColumn fx:id="nameColumn" text="Név" />
            <TableColumn fx:id="classColumn" text="Osztály" />
            <TableColumn fx:id="gradeColumn" text="Jegy" />
            <TableColumn fx:id="subjectColumn" text="Tantárgy" />
            <TableColumn fx:id="categoryColumn" text="Kategória" />
            <TableColumn fx:id="dateColumn" text="Dátum" />
        </columns>
    </TableView>
</VBox>
